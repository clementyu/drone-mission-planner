    <description>Mission including waypoints, a flight path, and a survey area.</description>

    <Style id="surveyAreaStyle">
      <LineStyle>
        <color>ff0000ff</color> <width>2</width>
      </LineStyle>
      <PolyStyle>
        <color>400000ff</color> </PolyStyle>
    </Style>

    <Placemark>
