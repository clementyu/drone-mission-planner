      <styleUrl>#surveyAreaStyle</styleUrl>
      <Polygon>
        <outerBoundaryIs>
          <LinearRing>
            <coordinates>
              -122.465,37.805,0
              -122.450,37.806,0
              -122.450,37.798,0
              -122.465,37.797,0
              -122.465,37.805,0
