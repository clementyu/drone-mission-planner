      <LineString>
        <coordinates>
          -122.476,37.807,100
          -122.478,37.819,120
          -122.480,37.830,100
        </coordinates>
      </LineString>
    </Placemark>

    <Placemark>
      <name>Waypoint 1: Fort Mason</name>
